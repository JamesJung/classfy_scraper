{
    "siteCode": "gwanak",
    "siteName": "관악구",
    "baseUrl": "https://www.gwanak.go.kr",
    "listUrl": "https://www.gwanak.go.kr/site/gwanak/ex/bbsNew/List.do?typeCode=1",
    "dateFormat": "YYYY.MM.DD",
    "selectors": {
        "list": "table",
        "row": "table tbody tr",
        "link": "td.subject a, td.tit a, td:nth-child(3) a",
        "title": "td.subject a, td.tit a, td:nth-child(3) a",
        "date": "td:nth-child(5)"
    },
    "pagination": {
        "type": "query",
        "param": "&pageIndex",
        "maxPages": 10
    },
    "onclickPatterns": [
        {
            "pattern": "doBbsFView\\(['\"]?(\\d+)['\"]?\\)",
            "urlTemplate": "https://www.gwanak.go.kr/site/gwanak/ex/bbsNew/View.do?not_ancmt_mgt_no={1}&typeCode={2}",
            "description": "doBbsFView 함수 패턴"
        },
        {
            "pattern": "doBbsFView",
            "urlTemplate": "https://www.gwanak.go.kr/site/gwanak/ex/bbsNew/View.do?not_ancmt_mgt_no={1}&typeCode={2}",
            "description": "doBbsFView 함수 패턴"
        }
    ],
    "announcementIdPattern": {
        "urlParams": [
            "not_ancmt_mgt_no"
        ],
        "description": "URL에서 공고 ID를 추출하는 파라미터 목록"
    },
    "dailyPages": 3
}