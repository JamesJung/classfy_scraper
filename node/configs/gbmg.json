            }
        }
    },
    "pagination": {
        "type": "query",
        "param": "&page",
        "maxPages": 10
    },
    "onclickPatterns": [],
    "announcementIdPattern": {
        "urlParams": [
            "notAncmtMgtNo"
        ],
