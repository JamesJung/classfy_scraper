{
    "siteCode": "boseong",
    "siteName": "보성군",
    "baseUrl": "https://www.boseong.go.kr",
    "listUrl": "https://www.boseong.go.kr/www/open_administration/city_news/notification",
    "dateFormat": "YYYY.MM.DD",
    "selectors": {
        "list": "table.board_t1",
        "row": "table.board_t1 tbody tr",
        "link": "td:nth-child(2) a",
        "title": "td:nth-child(2) a",
        "date": "td:nth-child(4)",
        "detail": {
            "content": "#board_basic_view",
            "attachments": {
                "selector": "a[href*='goDownLoad'], a[onclick*='goDownLoad']",
                "type": "javascript",
                "downloadFunction": "goDownLoad"
            }
        }
    },
    "pagination": {
        "type": "query",
        "param": "?page",
        "maxPages": 10
    },
    "onclickPatterns": [],
    "announcementIdPattern": {
        "urlParams": [
            "idx"
        ],
        "description": "URL에서 공고 ID를 추출하는 파라미터 목록"
    },
    "dailyPages": 3
}