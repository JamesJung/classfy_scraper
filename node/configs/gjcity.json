            "content": "div.bod_view",
            "attachments": {
                "selector": "a[href*='goDownload'], a[onclick*='goDownload']",
                "type": "javascript",
                "downloadFunction": "goDownload"
            }
        }
    },
    "pagination": {
        "type": "query",
        "param": "&page",
        "maxPages": 10
    },
    "onclickPatterns": [],
    "announcementIdPattern": {
        "urlParams": [
            "notAncmtMgtNo"
        ],
        "description": "URL에서 공고 ID를 추출하는 파라미터 목록"
    },
    "dailyPages": 3
