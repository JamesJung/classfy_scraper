{
    "siteCode": "anseong",
    "siteName": "안성시",
    "baseUrl": "https://www.anseong.go.kr",
    "listUrl": "https://www.anseong.go.kr/portal/saeol/gosiList.do?mId=0501040000&token=1757576243955",
    "dateFormat": "YYYY.MM.DD",
    "selectors": {
        "list": "table.tableSt_list",
        "row": "table.tableSt_list tbody tr",
        "link": "td:nth-child(2) a",
        "title": "td:nth-child(2) a",
        "date": "td:nth-child(4)",
        "detail": {
            "content": "#conts",
            "attachments": {
                "selector": "a[href*='goDownload'], a[onclick*='goDownload']",
                "type": "javascript",
                "downloadFunction": "goDownload"
            }
        }
    },
    "pagination": {
        "type": "javascript",
        "function": "goPage",
        "maxPages": 10
    },
    "onclickPatterns": [
        {
            "pattern": "boardView\\s*\\(\\s*['\"]([^'\"]+)['\"]\\s*,\\s*['\"]([^'\"]+)['\"]\\s*\\)",
            "urlTemplate": "https://www.anseong.go.kr/portal/saeol/gosiView.do?notAncmtMgtNo={2}&mId=0501040000",
            "description": "boardView(page, idx) 패턴"
        }
    ],
    "announcementIdPattern": {
        "urlParams": [
            "notAncmtMgtNo"
        ],
        "description": "URL에서 공고 ID를 추출하는 파라미터 목록"
    },
    "dailyPages": 3
}